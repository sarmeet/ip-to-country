{
  "name": "ip-to-country",
  "version": "1.0.0",
  "description": "API gateway for IP to country lookup",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "rimraf dist && tsc --build && copyfiles -u 1 src/**/*.json dist/",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.21.1",
    "helmet": "^5.1.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
